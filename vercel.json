{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.podoonyx.pl"
        },
        {
          "type": "protocol",
          "value": "http"
        }
      ],
      "destination": "https://www.podoonyx.pl/:path*",
      "permanent": true
    }
  ]
}
